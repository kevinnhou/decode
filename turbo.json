{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
            "env": ["GOOGLE_PRIVATE_KEY", "GOOGLE_CLIENT_ID", "GOOGLE_CLIENT_EMAIL"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"],
      "cache": false

    }
  }
}
